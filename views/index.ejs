<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/css/style.css' />
  <script src="http://code.jquery.com/jquery-1.10.2.min.js" type="text/javascript" ></script>
  <script src="js/jquery.ba-throttle-debounce.js" type="text/javscript" ></script>
  <script src="js/script.js" type="text/javascript" ></script>
  <script src="js/dropit.js"></script>
  <link rel="stylesheet" href="css/dropit.css" type="text/css" />
</head>
<body>

<!--<div id="loader">loading...</div>-->

<div id="nav">

  <h1><a href="#">VISUAL ARCHIVE</a></h1>

  <div id="info">
    <a href="https://github.com/quilime/visual-archive/blob/master/README.md">about</a>
  </div>

  <div id="filters">
    <% include filter-prelinger.ejs %>
    <div id="filter-elem">
      <input placeholder="Filter" type="text" id="filter" />
    </div>
  </div>

</div>


<ul id="thumbs" class="thumbs">
  <% for(var i = 0; i < clips.length; i++) { %>
    <li id="<%= clips[i].id %>" data-thumb-src="<%= clips[i].thumb_imgsrc %>">
      <a target="_blank" href="<%= clips[i].thumbnails_url %>"></a>
    </li>
  <% } %>
</ul>


<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-46753361-1', 'galadu.com');
  ga('send', 'pageview');
</script>

</body>
</html>
